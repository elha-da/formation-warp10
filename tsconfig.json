{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es2015", "es2016"],
    "jsx": "react",
    "jsxFactory": "kaiju.h",
    "module": "es2015",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5",
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "pretty": true,
    "sourceMap": true,
    "outDir": "./dist/"
  }
}